extends layout

block content
  .row.text-center
    if user
      a(href="/dashboard")
        h2 Dashboard 
      a(href="/link")
        h2 Link Apps
      a(href="/account")
        h2 Account

    else
      .col-sm-offset-3.col-sm-3
        a(href="/login")
          h1 Log In 
      .col-sm-3
        a(href="/signup")
          h1 Sign Up 